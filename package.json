{
  "name": "vue-library-seed",
  "version": "0.2.0",
  "description": "Seed project for Vue 2.x component libraries.",
  "main": "dist/vue-library-seed.js",
  "repository": {
    "url": "git@github.com:schuijers/vue-library-seed.git",
    "type": "git"
  },
  "author": "Martijn Schuijers",
  "license": "MIT",
  "dependencies": {
    "vue": "~2.1.3"
  },
  "devDependencies": {
    "@types/chai": "~3.4.34",
    "@types/core-js": "~0.9.35",
    "@types/mocha": "~2.2.33",
    "av-ts": "~0.5.5",
    "awesome-typescript-loader": "~3.0.0-beta.9",
    "babel-eslint": "~7.1.1",
    "chai": "~3.5.0",
    "css-loader": "~0.26.0",
    "del-cli": "~0.2.0",
    "eslint": "~3.11.1",
    "extract-text-webpack-plugin": "~v2.0.0-beta.4",
    "html-webpack-plugin": "~2.24.0",
    "karma": "~1.3.0",
    "karma-chai": "~0.1.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.1",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-typescript": "~2.1.4",
    "mocha": "~3.2.0",
    "npm-run-all": "~3.1.2",
    "postcss-loader": "~1.1.1",
    "stylelint": "~7.6.0",
    "stylelint-config-standard": "~15.0.0",
    "tslint": "~4.0.2",
    "tslint-loader": "~3.2.1",
    "typescript": "~2.0.10",
    "vue-html-loader": "~1.2.3",
    "vue-loader": "~10.0.2",
    "vue-style-loader": "~1.0.0",
    "vue-template-compiler": "~2.1.3",
    "webpack": "~2.1.0-beta.27",
    "webpack-dev-server": "~2.1.0-beta.12",
    "webpack-merge": "~1.0.2"
  },
  "scripts": {
    "build": "npm-run-all build:dist",
    "build:dist": "webpack --env.dist",
    "prebuild:dist": "npm-run-all clean lint test",
    "build:prod": "webpack --env.prod",
    "prebuild:prod": "npm-run-all clean lint test",
    "clean": "del dist/*",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint **/*.css",
    "lint:js": "eslint .",
    "lint:ts": "tslint -e node_modules **/*.ts",
    "serve": "webpack-dev-server --env.dev",
    "start": "npm-run-all serve",
    "test": "karma start",
    "test:local": "karma start --no-single-run"
  }
}
